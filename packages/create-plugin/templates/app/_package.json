{
  "name": "{{ kebabCase pluginId }}",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "webpack -c ../../.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ../../.config/webpack/webpack.config.ts --env development",
    "typecheck": "tsc --noEmit"
  },
  "author": "{{ sentenceCase orgName }}",
  "license": "Apache-2.0",
  "devDependencies": {
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.4",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@grafana/data": "{{ grafanaVersion }}",
    "@grafana/runtime": "{{ grafanaVersion }}",
    "@grafana/ui": "{{ grafanaVersion }}",{{#if_eq pluginType "scenesapp"}}
    "@grafana/schema": "{{ grafanaVersion }}",
    "@grafana/scenes": "0.27.0",{{/if_eq}}
    "react": "17.0.2",
    "react-dom": "17.0.2",{{#if isAppType}}
    "react-router-dom": "^5.2.0",
    "rxjs": "7.8.0",{{/if}}
    "tslib": "2.5.3"
  },
  "packageManager": "{{ packageManagerName }}@{{ packageManagerVersion }}"
}
